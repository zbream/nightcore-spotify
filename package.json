{
  "name": "nightcore-spotify",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev -- --watch",
    "build": "npm run build:prod",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "lint": "tslint -p .",
    "clean": "rimraf dist dist-artifacts",
    "ci": "run-s clean lint build web-ext:build",
    "web-ext:build": "web-ext build --source-dir dist --artifacts-dir dist-artifacts",
    "web-ext:run": "web-ext run --source-dir dist --url https://open.spotify.com --browser-console",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@types/chrome": "0.0.91",
    "@types/firefox-webext-browser": "^70.0.1",
    "@types/jest": "^24.0.23",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "web-ext": "^3.2.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "zbream-standards": "0.0.0-beta.11"
  }
}
